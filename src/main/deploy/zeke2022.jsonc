            "fire-action" : false,
            "shooter-w1" : false,
            "shooter-w2" : false,
            "shooter-h" : false,            
            "turret" : false,

            // Climber related
            "climber": true,
            "climber-model": true,

            // OI
            "zekeoi" : false
